{
  "name": "oit-canvas-ppsft-lti-enroll-observer",
  "version": "0.0.1",
  "scripts": {
    "build": "cd client && webpack -p --progress --colors --watch",
    "buildDev": "cd client && webpack --progress --colors --watch",
    "eslint": "esw --watch",
    "eslint:ci": "esw -o coverage/eslint.json -f json || echo \"ok eslint\"",
    "test": "NODE_ENV=test && BABEL_DISABLE_CACHE=1 jest ",
    "test:ci": "jest --testResultsProcessor ./node_modules/jest-junit || echo \"Ok tests\"",
    "testDebug": "NODE_ENV=test node --inspect-brk=0.0.0.0:8443 node_modules/.bin/jest",
    "reports:ci": "node node_modules/oit-canvas-jenkins-instrument/reports/index.js || echo \"ok reports\""
  },
  "dependencies": {
    "@instructure/ui-buttons": "^5.7.0",
    "@instructure/ui-elements": "^5.7.0",
    "@instructure/ui-forms": "^5.7.0",
    "@instructure/ui-icons": "^5.7.0",
    "@instructure/ui-menu": "^5.7.0",
    "@instructure/ui-themes": "^5.7.0",
    "@instructure/ui-overlays": "^5.7.0",
    "axios": "^0.16.2",
    "body-parser": "^1.18.2",
    "canvas-lms-api": "^0.2.0",
    "connect-history-api-fallback": "^1.3.0",
    "convict": "^1.5.0",
    "express": "^4.15.5",
    "express-jwt": "^5.3.0",
    "express-session": "^1.15.6",
    "history": "^4.4.1",
    "ims-lti": "^3.0.2",
    "jade": "^1.11.0",
    "jquery": "^3.3.1",
    "jquery-ui": "^1.12.1",
    "jquery-ui-bundle": "^1.12.1-migrate",
    "jsonwebtoken": "^8.1.0",
    "moment": "^2.21.0",
    "morgan": "^1.9.0",
    "nsp": "^3.2.1",
    "passport": "^0.4.0",
    "passport-lti": "0.0.7",
    "supertest": "^3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-jest": "^20.0.3",
    "babel-loader": "^6.2.8",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.22.0",
    "babel-regenerator-runtime": "^6.5.0",
    "eslint": "^3.6.1",
    "eslint-config-semistandard": "^11.0.0",
    "eslint-config-standard": "6.1.0",
    "eslint-config-standard-jsx": "3.1.0",
    "eslint-config-standard-react": "4.1.0",
    "eslint-loader": "1.3.0",
    "eslint-plugin-promise": "2.0.1",
    "eslint-plugin-react": "6.3.0",
    "eslint-plugin-standard": "2.0.0",
    "eslint-watch": "^3.1.3",
    "html-to-json": "^0.6.0",
    "http-server": "^0.10.0",
    "jest": "^22.4.3",
    "jest-cli": "^22.4.3",
    "jest-junit": "^4.0.0",
    "node-eval": "^2.0.0",
    "oit-canvas-jenkins-instrument": "https://artifactory.umn.edu/artifactory/oit-att-npm-public-local/oit-canvas-jenkins-instrument/-/oit-canvas-jenkins-instrument-1.0.0.tgz",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^4.4.6",
    "react-style-proptype": "^2.0.0",
    "react-test-renderer": "^16.4.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "semistandard": "^11.0.0",
    "supertest-session": "^3.1.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.4"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "collectCoverage": true,
    "coverageReporters": [
      "html",
      "cobertura",
      "json-summary"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/fixture/**",
      "!**/coverage/**",
      "!**/client/webpack.config.js",
      "!**/client/src/history.js",
      "!**/client/src/routes.js",
      "!**/client/src/router.js",
      "!**/client/src/app.js",
      "!**/client/build/**"
    ],
    "setupFiles": [
      "./__mocks__/init.js"
    ]
  },
  "jest-junit": {
    "output": "coverage/junit.xml"
  }
}
